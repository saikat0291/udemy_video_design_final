<div class="row">
 <div class="col-md-4 col-md-offset-4">
 <div class="well">
<%= simple_form_for(@contact_u) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email, required: true, autofocus: false %>
    <%= f.input :message %>
  </div>

  <div class="form-actions">
    <%= f.button :submit,"Submit",class: "btn btn-info" %>
  </div>
<% end %>
</div>
 </div>
</div>
